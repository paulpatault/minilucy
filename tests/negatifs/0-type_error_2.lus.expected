
/**************************************/
/* Sugar automaton print              */
/**************************************/
type inductive_bool =
  | False
  | True
type t =
  | A
  | B
  | C

node main0(x : adt(t)) returns (o : bool)
var ;
let
  (o) = merge x 
          (t(A) -> true when A(x))
          (t(B) -> 1 when B(x))
          (t(C) -> false when C(x));
tel

